<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="9.1%p">

    <Row
        android:keyWidth="9.99%p"
        android:rowEdgeFlags="top">
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="1" />
        <Key
            android:keyLabel="2" />
        <Key
           android:keyLabel="3" />
        <Key

            android:keyLabel="4" />
        <Key

            android:keyLabel="5" />
        <Key
            android:keyLabel="6" />
        <Key
            android:keyLabel="7" />
        <Key
            android:keyLabel="8" />
        <Key
            android:keyLabel="9" />
        <Key
            android:keyEdgeFlags="right"
            android:keyLabel="0" />
    </Row>
    <Row
        android:keyWidth="8.4%p">
        <Key
            android:keyEdgeFlags="left"
            android:keyLabel="й"
            android:popupCharacters="1"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="ц"
            android:popupCharacters="2"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="у"
            android:popupCharacters="3"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="к"
            android:popupCharacters="4"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="е"
            android:popupCharacters="5"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="н"
            android:popupCharacters="6"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="г"
            android:popupCharacters="7ґ"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="ґ"
            android:popupCharacters="7ґ"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="ш"
            android:popupCharacters="8"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="щ"
            android:popupCharacters="9"
            android:popupKeyboard="@xml/kbd_popup_template" />
        <Key
            android:keyLabel="з"
            android:popupCharacters="0"
            android:popupKeyboard="@xml/kbd_popup_template" />

        <Key android:keyLabel="х" />
